    "main": "build/src/index.js",
    "scripts": {
        "build": "tsc --project ./",
        "start:grader": "node ./build/src/grader/index.js",
        "start:server": "node ./build/src/server/index.js",
        "start": "echo \"Error: run start:server or start:grader instead \" && exit 1 # && concurrently \"redis-server ./redis.conf\" \"node ./build/src/server.js\" \"node ./build/src/queue.js\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        "@types/adm-zip": "^0.4.34",
        "@types/bull": "^3.15.2",
        "@types/express": "^4.17.9",
        "@types/morgan": "^1.9.2",
