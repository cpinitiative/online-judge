        "@typescript-eslint/eslint-plugin": "^4.10.0",
        "@typescript-eslint/parser": "^4.10.0",
        "adm-zip": "^0.5.5",
        "axios": "^0.21.1",
        "bull": "^3.26.0",
        "concurrently": "^6.2.0",
        "dotenv": "^10.0.0",
        "eslint": "^7.15.0",
        "eslint-config-prettier": "^7.0.0",
        "express": "^4.17.1",
        "firebase-admin": "^9.10.0",
        "https": "^1.0.0",
        "husky": "^4.3.6",
        "lint-staged": "^10.5.3",
